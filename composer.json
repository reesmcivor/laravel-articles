{
    "name": "reesmcivor/laravel-articles",
    "description": "Laravel Articles",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rees McIvor",
            "email": "reesmcivor@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "marcocesarato/php-conventional-changelog": "^1.17",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ReesMcIvor\\Articles\\": "src/",
            "ReesMcIvor\\Articles\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReesMcIvor\\Articles\\ArticlesPackageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "version": "0.0.1"
}
